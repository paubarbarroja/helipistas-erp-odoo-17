<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        
        <record model="ir.ui.view" id="leulit_20200629_1051_form">
            <field name="name">leulit_20200629_1051_form</field>
            <field name="model">leulit.modelohelicoptero</field>
            <field name="arch" type="xml">
                <form string="Model Helicoptero">
                    <sheet>
                        <group col="2">
                            <field name="name"/>
                            <field name="tipo"/>
                            <field name="descripcion"/>
                            <field name="pesomax"/>
                            <field name="performance_altura_velocidad"/>
                        </group>
                        <notebook colspan="4">
                            <page string="Helicopteros"> 
                                <field name="helicoptero_ids" invisible="1"/> 
                                <field name="helinobaja" colspan="4" nolabel="1">
                                    <tree string="Helicopteros" create="false" delete="false">
                                        <field name="name"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
		
	    <record model="ir.ui.view" id="leulit_20200629_1051_tree">
            <field name="name">leulit_20200629_1051_tree</field>
            <field name="model">leulit.modelohelicoptero</field>
            <field name="arch" type="xml">
                <tree string="Modelos helicopteros">
                    <field name="name" />
                    <field name="tipo" />
                    <field name="pesomax"/>
                    <field name="descripcion" />
                </tree>
            </field>
        </record>
		
		
        <record model="ir.actions.act_window" id="leulit_20200629_1051_action">
            <field name="name">Modelos helicopteros</field>
            <field name="res_model">leulit.modelohelicoptero</field>
            <field name="view_mode">tree,form</field>
        </record>

        
    </data>
</odoo>