<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="leulit_performance_EC120B_form" model="ir.ui.view">
            <field name="name">leulit.performance_EC120B_form</field>
            <field name="model">leulit.performance</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Performance EC120B" js_class="performance_view">
                    <group class="d-flex align-items-center">
                        <group col="2" colspan="2">
                            <group>
                                <field name="peso" widget="keyboard_disabled"/>
                            </group>
                            <group>
                                <field name="temperatura"/>
                            </group>
                        </group>
                        <div>
                            <button class="oe_highlight m-1 calcular_button" string="Calcular"/>
                            <button name="save_performance" type="object" string="Guardar" class="oe_highlight m-1" context="{'temperatura':temperatura}"/>
                            <field name="vuelo" invisible="1"/>
                            <field name="id" invisible="1"/>
                            <field name="ige" invisible="1"/>
                            <field name="oge" invisible="1"/>
                        </div>
                    </group>
                    <notebook>
                        <page string="IN GROUND EFFECT">
                            <div style="height: 500px; overflow:scroll; border:1px solid #000000;" id="micanvas_ec_in_div">
                            </div>
                        </page>
                        <page string="OUT GROUND EFFECT">
                            <div style="height: 500px; overflow:scroll; border:1px solid #000000;" id="micanvas_ec_out_div">
                            </div>
                        </page>
                    </notebook>
                </form>
            </field>
        </record>

